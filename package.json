{
  "name": "alia",
  "version": "0.17.0",
  "description": "Alias To Go",
  "exports": null,
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "al": "dist/main.js"
  },
  "scripts": {
    "start": "tsx src/main.ts",
    "prebuild": "del-cli dist/",
    "build": "tsc --build tsconfig.build.json",
    "postbuild": "del-cli dist/models",
    "test": "tsx test.ts",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "c8 --all npm run test",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write **/*.ts",
    "prepare": "husky install"
  },
  "keywords": [
    "alias",
    "cli",
    "cmd",
    "aka",
    "alias-to-go"
  ],
  "author": "Eduard Kotkas",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edkotkas/alia.git"
  },
  "bugs": {
    "url": "https://github.com/edkotkas/alia/issues"
  },
  "homepage": "https://github.com/edkotkas/alia#readme",
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/jasmine": "^5.1.0",
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "c8": "^8.0.1",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "http-request-mock": "^1.8.17",
    "husky": "^8.0.3",
    "jasmine": "^5.1.0",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  }
}
