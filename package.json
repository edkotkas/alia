{
  "name": "alia",
  "version": "0.22.0",
  "description": "Alias To Go",
  "exports": null,
  "type": "module",
  "files": [
    "dist/**/**",
    "data/**/**",
    "!dist/models/**",
    "!**/*.spec.*",
    "!**/*.tsbuildinfo",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "al": "dist/main.js"
  },
  "scripts": {
    "start": "tsx src/main.ts",
    "dist": "node dist/main.js",
    "snap": "tsx src/main.ts -h > snapshots/help",
    "prebuild": "del-cli dist/",
    "build": "tsc --build tsconfig.build.json",
    "test": "tsx test.ts",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "c8 --all npm run test",
    "test:mutation": "stryker run",
    "lint": "eslint .",
    "format": "prettier --config .prettierrc --ignore-unknown src",
    "format:fix": "npm run format -- --write",
    "format:check": "npm run format -- --check"
  },
  "keywords": [
    "alias",
    "cli",
    "cmd",
    "aka",
    "alias-to-go",
    "dev"
  ],
  "author": "Eduard Kotkas",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edkotkas/alia.git"
  },
  "bugs": {
    "url": "https://github.com/edkotkas/alia/issues"
  },
  "homepage": "https://github.com/edkotkas/alia#readme",
  "dependencies": {
    "dotenv": "^17.2.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.0.0",
    "@stryker-mutator/core": "^9.5.1",
    "@stryker-mutator/jasmine-runner": "^9.5.1",
    "@stryker-mutator/typescript-checker": "^9.5.1",
    "@types/jasmine": "^6.0.0",
    "@types/node": "^25.2.3",
    "c8": "^10.1.3",
    "del-cli": "^7.0.0",
    "eslint": "^9.0.0",
    "jasmine": "^6.0.0",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  }
}
